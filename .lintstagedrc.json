{
    "*.{html,json,css,scss,gql,js,ts,jsx,tsx,cjs,mjs}": [
        "bunx prettier --config ./prettier.config.cjs --write"
    ],
    "*.{yaml,yml}": [
        "bunx prettier --config ./prettier.config.cjs --tab-width 2 --write"
    ],
    "*.go": ["gofmt -w"],
    "*.md": ["bunx markdownlint-cli2 --fix"]
}
